{
  "name": "Gcopy Plus",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Gmail template",
  "homepage_url": "https://www.google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "options_page": "html/options.html",
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Gcopy",
    "default_popup": "html/browser_action.html"
  },
  "permissions": [
    "activeTab",
    "contentSettings",
    "<all_urls>"
  ]
}